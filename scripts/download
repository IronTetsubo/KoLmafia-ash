    if (immersion_adcode) {
        var container = jQuery('#content-immersion');
        container.show();
        jQuery('#content-template2').hide();
        
        var now = new Date(), october = new Date(2012, 9, 31);
        if (now > october) {
            if ((immersion_adcode.indexOf('accelacomm') === -1 &&
                 immersion_adcode.indexOf('perfectforms') === -1) &&
                 container.children('.sidebar').length &&
                (container.find('.sidebar .accordion').length ||
                 container.find('.sidebar .sidebar-widget').css('display') === 'block')) {
                container.addClass('with-sidebar');
                container.children('.sidebar').show();
            }
        }
        jQuery('body').attr('data-template', 'template1');
    } else {
        jQuery('body').attr('data-template', 'template2');
        var container = jQuery('#content-template2');
        if (container.children('.sidebar').length &&
            (container.find('.sidebar .accordion').length ||
            container.find('.sidebar .sidebar-widget').css('display') === 'block')) {
            container.addClass('with-sidebar');
            container.children('.sidebar').show();
        }
    }
